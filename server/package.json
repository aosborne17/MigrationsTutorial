{
  "name": "MigrationsTutorial",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "make-user-model": "cd src && npx sequelize-cli model:generate --name User --attributes firstName:string,lastName:string,email:string",
    "migrate": "cd src && npx sequelize-cli db:migrate",
    "undo-migrate": "npx sequelize-cli db:migrate:undo",
    "make-user-seed": "npx sequelize-cli seed:generate --name demo-user",
    "seed-all": "npx sequelize-cli db:seed:all",
    "start": "env NODE_ENV=production npm run migrate && node src/index.js",
    "dev": "set env NODE_ENV=development npm run migrate && nodemon src/index.ts",
    "test": "npm run migrate && jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.2",
    "mysql2": "^2.2.5",
    "redis": "^3.1.2",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.4"
  }
}
